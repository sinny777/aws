<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<bean id="salesforce-connector"
		class="com.google.enterprise.connector.salesforce.BaseConnector">
		      <constructor-arg index="0" value="${username}"/>
		      <constructor-arg index="1" value="${password}"/>
		      <constructor-arg index="2" value="${schedule}"/>
		      <constructor-arg index="3" value="${query}"/>
		      <constructor-arg index="4" value="${xslt}"/>
		      <constructor-arg index="5" value="${azquery}"/>
		      <constructor-arg index="6" value="${azxslt}"/>
		      <constructor-arg index="7" value="${loginsrv}"/>
		      <constructor-arg index="8" value="${lastsync}"/>
   		      <constructor-arg index="9" value="${storetype}"/>
		      <constructor-arg index="10" value="${googleConnectorWorkDir}"/>


			    <property name="username" value="${username}" />
	    	    <property name="password" value="${password}" />
	    	    <property name="loginsrv" value="${loginsrv}" />
	    	    <property name="lastsync" value="${lastsync}" />
			    <property name="schedule" value="${schedule}" />
	    	    <property name="query" value="${query}" />
	    	    <property name="xslt" value="${xslt}" />
	    	    <property name="azquery" value="${azquery}" />	 
	    	    <property name="azxslt" value="${azxslt}" />     
	    	    <property name="storetype" value="${storetype}"/>  
	    	    <property name="googleConnectorWorkDir" value="${googleConnectorWorkDir}"/>        	    
	</bean>


</beans>
